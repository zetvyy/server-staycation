<div
  class="tab-pane fade show active ml-2"
  id="show-image"
  role="tabpanel"
  aria-labelledby="home-tab"
  tabindex="0"
>
  <div class="row">
    <% for (let j = 0; j < item.imageId.length; j++){ %>
    <div class="col-md-4">
      <div class="card shadow mb-4 mt-2">
        <div class="card-body">
          <img
            src="http://localhost:8000/<%= item.imageId[j].imageUrl %>"
            alt="image"
            style="width: 100%; display: block"
          />
        </div>
      </div>
    </div>
    <% } %>
  </div>
  <a href="/admin/item" class="btn btn-warning">Back</a>
</div>
